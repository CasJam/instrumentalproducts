{{ if video | is_embeddable }}
    <iframe
        src="{{ video | embed_url }}"
        class="my-12 aspect-video w-full overflow-hidden rounded-lg"
    ></iframe>
{{ else }}
    <video
        src="{{ video | embed_url }}"
        class="my-12 aspect-video w-full overflow-hidden rounded-lg"
    ></video>
{{ /if }}